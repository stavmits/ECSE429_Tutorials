[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/Stavros Mitsoglou (stavmits)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Stavros Mitsoglou (stavmits)
	label-Verified = -1..+1 group user/Stavros Mitsoglou (stavmits)
	push = group user/Stavros Mitsoglou (stavmits)
	read = group user/Stavros Mitsoglou (stavmits)
	rebase = group user/Stavros Mitsoglou (stavmits)
	removeReviewer = group user/Stavros Mitsoglou (stavmits)
	submit = group user/Stavros Mitsoglou (stavmits)
[access "refs/heads/*"]
	create = group user/Stavros Mitsoglou (stavmits)
	push = +force group user/Stavros Mitsoglou (stavmits)
	pushMerge = group user/Stavros Mitsoglou (stavmits)
	read = group user/Stavros Mitsoglou (stavmits)
[access "refs/tags/*"]
	push = group user/Stavros Mitsoglou (stavmits)
	push = +force group user/Stavros Mitsoglou (stavmits)
